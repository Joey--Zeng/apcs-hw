public class Node {
    private int data;
    private Node left,right;
    
    public Node(int a){
	data = a;
    }

    public void setData(int a){
	data = a;
    }
    
    public int getData(){
	return data;
    }

    public Node getLeft(){
	return left;
    }

    public void setLeft(int a){
	left = a;
    }

    public int getRight(){
	return right;
    }

    public void setRight(int a){
        right = a;
    }
	
    // write all necessary constructors and get/set methods
}
